
platform :ios, '11.0'

xcodeproj 'TraktDemo', 'Prod' => :release, 'Dev' => :debug, 'Mocks' => :debug

target 'TraktDemo' do
  use_frameworks!

  pod 'AFNetworking'
  pod 'AFNetworkActivityLogger', :git => 'https://github.com/AFNetworking/AFNetworkActivityLogger.git'
  pod 'ISO8601'
  pod 'PSTAlertController'
  pod 'DateTools'
  pod 'Shimmer'
  pod 'Reachability'
  pod 'SDWebImage'
  pod 'CocoaLumberjack'

end
